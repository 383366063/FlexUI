<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.ButtonBar")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<fx:Declarations>
		<fx:Component id="firstButton">
			<s:ButtonBarButton width="100%" skinClass="net.lanelife.pc.webos.soft.theme.skins.ButtonBarButtonSkin" />
		</fx:Component>
		<fx:Component id="middleButton" >
			<s:ButtonBarButton width="100%" skinClass="net.lanelife.pc.webos.soft.theme.skins.ButtonBarButtonSkin" />
		</fx:Component>
		<fx:Component id="lastButton" >
			<s:ButtonBarButton width="100%" skinClass="net.lanelife.pc.webos.soft.theme.skins.ButtonBarButtonSkin" />
		</fx:Component>
	</fx:Declarations>
	
	<s:DataGroup id="dataGroup" width="100%" height="100%">
		<s:layout>
			<s:VerticalLayout gap="0" />
		</s:layout>
	</s:DataGroup>
	
	<!-- SkinParts
	name=lastButton, type=mx.core.IFactory, required=false
	name=firstButton, type=mx.core.IFactory, required=false
	name=middleButton, type=mx.core.IFactory, required=true
	name=dataGroup, type=spark.components.DataGroup, required=false
	-->
</s:Skin>
