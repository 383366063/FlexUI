<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="100">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("net.lanelife.framework.os.components.ScreenNavigator")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<s:Rect radiusX="12" height="24" left="0" right="0">
		<s:stroke>
			<s:SolidColorStroke color="#ffffff" alpha="0.6"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="#000000" alpha="0.25"/>
		</s:fill>
	</s:Rect>
	
	<s:DataGroup dataProvider="{hostComponent.navigatorButtons}" verticalCenter="0" horizontalCenter="0">
		<s:layout>
			<s:HorizontalLayout paddingLeft="20" paddingRight="20"/>
		</s:layout>
	</s:DataGroup>
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	-->
</s:Skin>
